{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "array",
    "uniqueItems": true,
    "items": {
        "type": "object",
        "properties": {
            "ParameterKey": {
                "type": "string",
                "pattern": "^[\\w\\d]*$"
            },
            "ParameterValue": { "oneOf": [ {"type": "string"}, {"type": "boolean"}, {"type": "integer"} ] }
        },
        "required": [
            "ParameterKey",
            "ParameterValue"
        ],
        "additionalProperties": false
    }
}
