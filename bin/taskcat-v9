from __future__ import print_function
import sys


if __name__ == '__main__':
    py_ver = float(str(sys.version_info[0]) + '.' + str(sys.version_info[1]))
    if py_ver < 3.6:
        print("taskcat requires python 3.6 or higher")
        sys.exit(1)
    from taskcat.cli import main
    main()
